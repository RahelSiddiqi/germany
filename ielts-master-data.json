  "writingTemplates": {
    "task1": {
      "description": "Academic Writing Task 1 Templates",
      "types": [
        {
          "type": "Line Graph",
          "intro": "The line graph illustrates/shows/depicts [what] [where] [when].",
          "overview": "Overall, it is clear that [main trend 1] while [main trend 2].",
          "keyPhrases": ["increased significantly", "decreased gradually", "remained stable", "fluctuated", "peaked at", "reached a low of", "from X to Y"]
        },
        {
          "type": "Bar Chart",
          "intro": "The bar chart compares [what] in [where] [when].",
          "overview": "Overall, [highest category] had the highest [measure] while [lowest category] had the lowest.",
          "keyPhrases": ["the largest proportion", "accounted for", "stood at", "compared to", "in contrast"]
        },
        {
          "type": "Pie Chart",
          "intro": "The pie chart(s) show(s) the proportion of [what] [where] [when].",
          "overview": "Overall, [dominant segment] made up the majority while [smallest] represented the smallest share.",
          "keyPhrases": ["comprised", "constituted", "made up", "represented", "the majority", "a quarter", "a third"]
        },
        {
          "type": "Table",
          "intro": "The table provides information about [what] [where] [when].",
          "overview": "Overall, it can be seen that [main observation 1] and [main observation 2].",
          "keyPhrases": ["the highest figure", "the lowest figure", "compared to", "while", "whereas"]
        },
        {
          "type": "Process Diagram",
          "intro": "The diagram illustrates the process of [what]/how [something] is made.",
          "overview": "Overall, there are [X] main stages in the process, beginning with [first stage] and ending with [final stage].",
          "keyPhrases": ["At the first stage", "Following this", "Subsequently", "Finally", "is then", "after which"]
        },
        {
          "type": "Map",
          "intro": "The maps compare [what] in [when: past vs present/proposed].",
          "overview": "Overall, the area has undergone significant changes, particularly in [major change].",
          "keyPhrases": ["has been replaced by", "was converted into", "remains unchanged", "in the north/south/east/west", "adjacent to", "opposite"]
        }
      ]
    },
    "task2": {
      "description": "Academic Writing Task 2 Templates",
      "types": [
        {
          "type": "Opinion Essay",
          "question": "To what extent do you agree or disagree?",
          "structure": ["Introduction (paraphrase + thesis)", "Body 1 (main reason + example)", "Body 2 (second reason + example)", "Conclusion (restate opinion)"],
          "phrases": {
            "agree": ["I strongly believe that", "In my opinion", "From my perspective", "I am convinced that"],
            "disagree": ["I completely disagree with the notion that", "I do not believe that", "This view is not supported by evidence"]
          }
        },
        {
          "type": "Discussion Essay",
          "question": "Discuss both views and give your opinion.",
          "structure": ["Introduction (paraphrase + thesis)", "Body 1 (View 1 + reasons)", "Body 2 (View 2 + reasons)", "Body 3 (Your opinion)", "Conclusion"],
          "phrases": ["On the one hand", "On the other hand", "However", "Nevertheless", "In contrast", "Despite this"]
        },
        {
          "type": "Problem-Solution Essay",
          "question": "What are the problems? What solutions can you suggest?",
          "structure": ["Introduction (paraphrase + overview)", "Body 1 (Problems)", "Body 2 (Solutions)", "Conclusion"],
          "phrases": ["One of the main problems is", "This leads to", "A possible solution would be", "This could be addressed by", "Governments should"]
        },
        {
          "type": "Advantages-Disadvantages Essay",
          "question": "What are the advantages and disadvantages?",
          "structure": ["Introduction (paraphrase + thesis)", "Body 1 (Advantages)", "Body 2 (Disadvantages)", "Conclusion (overall view)"],
          "phrases": ["The main advantage is", "Another benefit is", "However, there are drawbacks", "The most significant disadvantage is", "On balance"]
        },
        {
          "type": "Two-Part Question",
          "question": "Why is this? What can be done?",
          "structure": ["Introduction (paraphrase)", "Body 1 (Answer part 1)", "Body 2 (Answer part 2)", "Conclusion"],
          "phrases": ["There are several reasons for this", "The main cause is", "To address this issue", "Possible measures include"]
        }
      ]
    }
  },
